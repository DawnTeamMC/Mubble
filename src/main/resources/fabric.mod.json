{
	"schemaVersion": 1,
	"name": "${mod_name}",
	"id": "${mod_id}",
	"version": "${version}",
	"icon": "assets/${mod_id}/textures/logo.png",
	"description": "Expand your world with features from the Nintendo universe, and way more.",
	"authors": [
		"Hugman",
		"Dawn Team"
	],
	"contributors": [
		"Yanis48"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/${curseforge_slug}",
		"sources": "https://github.com/DawnTeamMC/${github_name}",
		"issues": "https://github.com/DawnTeamMC/${github_name}/issues"
	},
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.hugman.mubble.Mubble"
		],
		"client": [
			"com.hugman.mubble.MubbleClient"
		],
		"modmenu": [
			"com.hugman.mubble.config.MubbleModMenuIntegration"
		]
	},
	"mixins": [
		"mubble.mixins.json"
	],
	"depends": {
		"minecraft": "*",
		"fabric": "*",
		"dawn": "*"
	},
	"custom": {
		"modupdater": {
			"strategy": "curseforge",
			"projectID": 390135
		}
	},
	"license": "MIT"
}
